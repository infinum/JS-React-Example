{
	"name": "@infinum/ui",
	"version": "0.1.0",
	"private": true,
	"sideEffects": false,
	"scripts": {
		"check-licenses": "node ../../scripts/check-licenses-workspace.js",
		"clean": "rm -rf node_modules .turbo .eslintcache",
		"lint": "eslint . --cache",
		"lint:fix": "eslint . --cache --fix",
		"prettier:check": "prettier --check .",
		"prettier:fix": "prettier --write .",
		"test": "jest --passWithNoTests",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"test:coverage:watch": "jest --coverage --watch"
	},
	"exports": {
		"./*": "./src/components/*.tsx",
		"./themes/*": "./src/themes/*",
		"./hooks/*": "./src/hooks/*.tsx",
		"./utils/*": "./src/utils/*"
	},
	"files": [
		"src",
		"src/themes/*"
	],
	"devDependencies": {
		"@infinum/configs": "workspace:*",
		"@storybook/nextjs": "catalog:",
		"@testing-library/dom": "catalog:",
		"@testing-library/jest-dom": "catalog:",
		"@testing-library/react": "catalog:",
		"@testing-library/user-event": "catalog:",
		"@types/jest": "catalog:",
		"@types/node": "catalog:",
		"@types/react": "catalog:",
		"@types/react-dom": "catalog:",
		"autoprefixer": "catalog:",
		"eslint": "catalog:",
		"jest": "catalog:",
		"jest-environment-jsdom": "catalog:",
		"postcss": "catalog:",
		"prettier": "catalog:",
		"prettier-plugin-sh": "catalog:",
		"prettier-plugin-tailwindcss": "catalog:",
		"react": "catalog:",
		"react-dom": "catalog:",
		"shadcn": "catalog:",
		"storybook": "catalog:",
		"tailwindcss": "catalog:",
		"ts-jest": "catalog:",
		"typescript": "catalog:"
	},
	"dependencies": {
		"@radix-ui/react-label": "catalog:",
		"@radix-ui/react-select": "catalog:",
		"@radix-ui/react-slot": "catalog:",
		"@radix-ui/react-tooltip": "catalog:",
		"class-variance-authority": "catalog:",
		"clsx": "catalog:",
		"lucide-react": "catalog:",
		"tailwind-merge": "catalog:",
		"tw-animate-css": "catalog:"
	}
}
