{
	"$schema": "https://turbo.build/schema.json",
	"ui": "stream",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env", ".env.local"],
			"outputs": [".next/**", "!.next/cache/**", "dist/**", "build/**", "storybook-static/**"]
		},
		"clean": {
			"cache": false
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"lint": {
			"dependsOn": ["^lint"],
			"inputs": ["**/*.{js,ts,jsx,tsx,mjs,cjs}"]
		},
		"lint:fix": {
			"dependsOn": ["^lint"],
			"inputs": ["**/*.{js,ts,jsx,tsx,mjs,cjs}"]
		},
		"//#lint:root": {
			"inputs": ["$TURBO_DEFAULT$", "!apps/**", "!packages/**"]
		},
		"//#lint:fix:root": {
			"inputs": ["$TURBO_DEFAULT$", "!apps/**", "!packages/**"]
		},
		"prettier:check": {},
		"prettier:fix": {},
		"//#prettier:check:root": {
			"inputs": ["$TURBO_DEFAULT$", "!apps/**", "!packages/**"]
		},
		"//#prettier:fix:root": {
			"inputs": ["$TURBO_DEFAULT$", "!apps/**", "!packages/**"]
		},
		"test": {
			"outputs": ["coverage/**"],
			"inputs": ["**/*.test.{ts,tsx}"]
		},
		"start": {
			"dependsOn": ["^build"],
			"cache": false,
			"persistent": true
		},
		"check-licenses": {
			"cache": false
		}
	}
}
