{
	"name": "@infinum/frontend",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"build": "next build",
		"build:analyze": "ANALYZE=true pnpm build",
		"check-licenses": "node ../../scripts/check-licenses-workspace.js",
		"clean": "rm -rf node_modules .turbo .next .eslintcache",
		"dev": "NODE_OPTIONS='--inspect' next dev",
		"lint": "eslint . --cache",
		"lint:fix": "eslint . --cache --fix",
		"prettier:check": "prettier --check .",
		"prettier:fix": "prettier --write .",
		"start": "next start",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"test:coverage:watch": "jest --coverage --watch"
	},
	"dependencies": {
		"@infinum/ui": "workspace:*",
		"@tailwindcss/postcss": "catalog:",
		"envsafe": "catalog:",
		"next": "catalog:",
		"next-auth": "catalog:",
		"next-intl": "catalog:",
		"next-public-env": "1.0.0",
		"next-themes": "catalog:",
		"postcss": "catalog:",
		"react": "catalog:",
		"react-dom": "catalog:",
		"tailwindcss": "catalog:"
	},
	"devDependencies": {
		"@infinum/configs": "workspace:*",
		"@next/bundle-analyzer": "catalog:",
		"@next/env": "catalog:",
		"@testing-library/dom": "catalog:",
		"@testing-library/jest-dom": "catalog:",
		"@testing-library/react": "catalog:",
		"@testing-library/user-event": "catalog:",
		"@types/jest": "catalog:",
		"@types/node": "catalog:",
		"@types/react": "catalog:",
		"@types/react-dom": "catalog:",
		"jest": "catalog:",
		"jest-environment-jsdom": "catalog:",
		"prettier": "catalog:",
		"prettier-plugin-sh": "catalog:",
		"prettier-plugin-tailwindcss": "catalog:",
		"ts-jest": "catalog:",
		"ts-node": "catalog:",
		"typescript": "catalog:"
	}
}
