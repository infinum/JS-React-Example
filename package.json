{
  "name": "js-react-example",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "PROXY_ENV=development next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest --coverage",
    "watch": "jest --watch --coverage",
    "api:dev": "tsnd --pretty --project ./api/tsconfig.json ./api",
    "storybook": "start-storybook -p 6006 -s ./public",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.6.3",
    "@datx/core": "^2.2.1",
    "@datx/jsonapi": "^2.2.0",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@hookform/error-message": "2.0.0",
    "date-fns": "^2.22.1",
    "dequal": "^2.0.2",
    "focus-visible": "^5.2.0",
    "framer-motion": "^4.1.17",
    "http-proxy-middleware": "^2.0.0",
    "mobx": "^4.15.7",
    "next": "10.2.3",
    "next-compose-plugins": "^2.2.1",
    "next-react-svg": "^1.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.6.10",
    "swr": "^0.5.6"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-proposal-decorators": "^7.14.2",
    "@infinumjs/eslint-config-react": "^2.1.0",
    "@storybook/addon-a11y": "^6.2.9",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.1",
    "@types/react": "^17.0.8",
    "babel-loader": "^8.2.2",
    "eslint": "^7.27.0",
    "eslint-plugin-import": "^2.23.3",
    "husky": "^4.3.8",
    "jest": "^27.0.1",
    "lint-staged": "^10.5.4",
    "msw": "^0.29.0",
    "msw-storybook-addon": "^1.1.0",
    "prettier": "^2.3.0",
    "storybook-addon-next-router": "^2.0.4",
    "storybook-addon-performance": "^0.15.2",
    "ts-jest": "^27.0.1",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.3.2"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test",
      "pre-commit": "prettier --write"
    }
  },
  "lint-staged": {
    "{src,tests}/**/*.{js,ts,tsx}": [
      "eslint",
      "prettier --write"
    ]
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/tests/",
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "tsx"
    ],
    "testRegex": "tests/(.*).test.tsx?$",
    "globals": {
      "ts-jest": {
        "diagnostics": {
          "warnOnly": true
        }
      }
    },
    "preset": "ts-jest",
    "testMatch": null
  }
}
