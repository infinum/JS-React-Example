{
	"name": "js-react-example",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"preinstall": "npx only-allow npm",
		"dev": "PROXY_ENV=development next dev",
		"build": "next build",
		"postbuild": "next-sitemap",
		"start": "next start",
		"lint": "eslint . --ext .js,.jsx,.ts,.tsx,.d.ts",
		"lint:next": "next lint",
		"test": "jest",
		"test:coverage": "jest --coverage",
		"test:watch": "jest --watch --coverage",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook",
		"analyze": "ANALYZE=true yarn build",
		"analyze:sourcemap": "source-map-explorer .next/static/**/*.js",
		"prepare": "husky install",
		"gen:theme-typings": "chakra-cli tokens ./src/styles/theme/index.ts --strict-component-types --strict-token-types",
		"gen:theme-typings:watch": "chakra-cli tokens ./src/styles/theme/index.ts --strict-component-types --strict-token-types --watch",
		"i18n:generate": "node ./scripts/i18next-cli.js",
		"gen:component": "plop component",
		"gen:theme": "plop theme",
		"postinstall": "npm run gen:theme-typings"
	},
	"dependencies": {
		"@bugsnag/js": "~7.20.0",
		"@bugsnag/plugin-aws-lambda": "~7.19.0",
		"@bugsnag/plugin-react": "~7.19.0",
		"@chakra-ui/react": "~2.5.1",
		"@chakra-ui/utils": "~2.0.15",
		"@datx/core": "~2.5.0-beta.6",
		"@datx/jsonapi": "~2.5.0-beta.6",
		"@datx/swr": "~2.5.0-beta.9",
		"@emotion/react": "~11.10.5",
		"@emotion/styled": "~11.10.5",
		"@hookform/error-message": "~2.0.1",
		"date-fns": "~2.29.3",
		"eslint-config-next": "~13.2.1",
		"focus-visible": "~5.2.0",
		"framer-motion": "~10.0.1",
		"http-proxy-middleware": "~2.0.6",
		"i18next": "~22.4.9",
		"mobx": "~6.7.0",
		"next": "~13.2.1",
		"next-i18next": "~13.1.5",
		"next-sitemap": "^3.1.43",
		"react": "~18.2.0",
		"react-dom": "~18.2.0",
		"react-hook-form": "~7.43.2",
		"react-i18next": "~12.2.0",
		"react-icons": "~4.7.1",
		"swr": "~2.0.0"
	},
	"devDependencies": {
		"@chakra-ui/cli": "~2.3.0",
		"@chakra-ui/storybook-addon": "~4.0.15",
		"@infinum/jest": "~1.0.0",
		"@infinum/plop-next-ts-generators": "~1.0.2",
		"@next/bundle-analyzer": "~13.2.1",
		"@next/eslint-plugin-next": "~13.2.1",
		"@storybook/addon-a11y": "~6.5.15",
		"@storybook/addon-actions": "~6.5.15",
		"@storybook/addon-essentials": "~6.5.15",
		"@storybook/addon-links": "~6.5.15",
		"@storybook/builder-webpack5": "~6.5.15",
		"@storybook/manager-webpack5": "~6.5.15",
		"@storybook/react": "~6.5.15",
		"@testing-library/jest-dom": "~5.16.5",
		"@testing-library/react": "~14.0.0",
		"@testing-library/user-event": "~14.4.3",
		"@types/jest": "~29.4.0",
		"@types/jest-axe": "~3.5.5",
		"@types/node": "~18.14.2",
		"@types/react": "~18.0.26",
		"@types/react-dom": "~18.0.9",
		"@typescript-eslint/eslint-plugin": "~5.53.0",
		"@typescript-eslint/parser": "~5.53.0",
		"eslint": "~8.35.0",
		"eslint-plugin-chakra-ui": "~0.7.6",
