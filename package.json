		"jest-environment-jsdom": "~29.4.2",
		"jest-watch-typeahead": "~2.2.2",
		"lint-staged": "~13.1.0",
		"msw": "~0.49.2",
		"msw-storybook-addon": "~1.7.0",
		"plop": "~3.1.1",
		"prettier": "~2.8.1",
		"source-map-explorer": "~2.5.3",
		"storybook-addon-next-router": "~4.0.2",
		"tsconfig-paths-webpack-plugin": "~4.0.0",
